        <client_script><![CDATA[$(function() {
	if(!hasAssistRole()){
		var no_access_component = assistNoAccessPage(assist_application_role_txt,assist_admin_contact_txt);
		alert(assist_application_role_txt+"\n"+assist_admin_contact_txt);
	}else{
		var case_id = $('#sys_id_input').val();
		var type = $('#sys_type_input').val();
		getIncidentRecord(case_id);
		var api_key = incident_detail.api_key;
		if(api_key!=undefined){
			getSessionAPI(type);
		}else{
			var not_configured = assistNoAccessPage(not_configured_txt,assist_admin_contact_txt);
			alert(not_configured_txt+"\n"+assist_admin_contact_txt);
		}
	}
	closeDialog();
});

function closeDialog(){
	GlideDialogWindow.get().destroy();
}]]></client_script>
